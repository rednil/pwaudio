{
  "name": "pwaudio",
  "version": "0.9.0",
  "contributors": [
    "The Polymer Authors, Christian Linder"
  ],
  "license": "BSD-3-Clause",
  "repository": "rednil/pwaudio",
  "scripts": {
    "watch": "browser-sync start --proxy localhost:8080 --open --files \"src/**/*.js\" \"index.html\"",
    "start": "npm run start:backend | npm run start:frontend",
    "start:frontend": "polymer serve --port 8080 --proxy-path=fs --proxy-target=http://localhost:3001/fs | npm run watch",
    "start:backend": "node server/index.js -v /mnt/teracrypt/audio/audiobooks/de -p 3001",
    "build": "npm run build:static",
    "build:prpl": "polymer build --auto-base-path && gulp prpl-server",
    "build:static": "polymer build",
    "serve": "prpl-server --root server/build",
    "serve:static": "polymer serve --port 5000 build/es5-bundled",
    "test": "",
    "test:bak": "npm run test:unit && npm run test:integration",
    "test:integration": "mocha test/integration --timeout=10000",
    "test:unit": "polymer test --module-resolution=node --npm",
    "test:sauce": "polymer test --module-resolution=node --npm -s 'windows 10/microsoftedge@17' -s 'macos 10.13/safari@11'",
    "test:regenerate_screenshots": "mocha test/integration/screenshots-baseline/regenerate.js --timeout=15000"
  },
  "dependencies": {
    "@webcomponents/webcomponentsjs": "^2.2.4",
    "browser-sync": "^2.26.7",
    "dexie": "^2.0.4",
    "lit-element": "^2.0.1",
    "pwa-helpers": "^0.9.0",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "webdav": "^2.10.0"
  },
  "devDependencies": {
    "polymer-cli": "^1.9.11",
    "axe-core": "^3.0.0",
    "chai": "^4.1.2",
    "del": "^3.0.0",
    "gulp": "^4.0.0",
    "gulp-rename": "^1.3.0",
    "gulp-replace": "^1.0.0",
    "mocha": "^5.2.0",
    "pixelmatch": "^4.0.2",
    "polyserve": "^0.27.0",
    "prpl-server": "^1.4.0",
    "wct-mocha": "^1.0.0"
  }
}
